<!DOCTYPE html>
<html ng-app="app">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{@message type="content" key="site.title"/} {+title /}</title>
  <!--Import Google Icon Font-->
  <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!--Import css-->
  <link type="text/css" rel="stylesheet" href="/stylesheets/application.min.css"  media="screen,projection"/>
</head>
<body>
  {>"layouts/side-nav" /}
  <main>
    {>"layouts/nav" /}
    {+fluidContainer /}
    <div class="container">
      {+body /}
    </div>

    {>"layouts/form-sign-up" /}
    {>"layouts/form-sign-in" /}
  </main>
  <!--Import js-->
  <script type="text/javascript" src="/javascripts/application.min.js"></script>
</body>
</html>
